{
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": "1080",
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": false
      },
      "sniffing": {
        "destOverride": [
          "tls",
          "http"
        ],
        "enabled": true
      }
    },
    {
      "listen": "0.0.0.0",
      "port": "12306",
      "protocol": "http",
      "settings": {
        "timeout": 360
      },
      "sniffing": {
        "destOverride": [
          "tls",
          "http"
        ],
        "enabled": true
      }
    }
  ],
  "outbounds": [
    {
      "mux": {
        "concurrency": 5,
        "enabled": true
      },
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "填入代理服务器的地址",
            "port": 80,
            "users": [
              {
                "alterId": 0,
                "id": "你的授权ID",
                "level": 0,
                "security": "aes-128-cfb"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "tlsSettings": {
          "allowInsecure": true
        },
        "wsSettings": {
          "headers": {
            "host": ""
          },
          "path": "/release"
        }
      },
      "tag": "proxy"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "block"
    },
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    }
  ]
}